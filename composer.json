{
    "name": "sofyco/doctrine-mongodb-document-validation-bundle",
    "type": "symfony-bundle",
    "version": "3.1.0",
    "description": "Doctrine ODM Validation Constraints",
    "keywords": ["php", "symfony", "bundle", "doctrine", "mongodb", "odm", "validation"],
    "homepage": "https://github.com/sofyco/doctrine-mongodb-document-validation-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Sofiia Korzhova",
            "email": "sofykorzhova@gmail.com"
        }
    ],
    "require": {
        "php": "^8.5",
        "ext-mongodb": "*",
        "doctrine/mongodb-odm-bundle": "^5.5",
        "symfony/framework-bundle": "^8.0",
        "symfony/security-bundle": "^8.0",
        "symfony/validator": "^8.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^2.1",
        "phpunit/phpunit": "^12.4"
    },
    "autoload": {
        "psr-4": {
            "Sofyco\\Bundle\\Doctrine\\MongoDB\\DocumentValidationBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sofyco\\Bundle\\Doctrine\\MongoDB\\DocumentValidationBundle\\Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
